{
  "name": "familiada",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "migrate": "pnpm --filter api run migrate:dev",
    "lint": "turbo run lint",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "turbo": "1.4.3"
  },
  "packageManager": "pnpm@7.8.0",
  "engines": {
    "node": "18.6.0"
  }
}
