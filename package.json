{
  "name": "familiada",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "2.7.1",
    "lint-staged": "13.0.3",
    "turbo": "1.3.4",
    "husky": "8.0.1"
  },
  "engines": {
    "node": "18.6.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.5.2"
}
